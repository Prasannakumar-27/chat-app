{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://chat-app-backend.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "client/dist"
}